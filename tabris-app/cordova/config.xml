<?xml version='1.0' encoding='utf-8'?>
<widget id="org.crossecore.examples.eclipsecon2018.tabris" version="0.1.0">
  <name>EclipseCon 2018</name>
  <description>
    Example Tabris.js App
  </description>
  <author email="schwicht@mail.upb.de">
    Simon Schwichtenberg
  </author>
  <plugin name="cordova-plugin-sqlite-2" spec="^1.0.5" />
  <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
  <preference name="EnableDeveloperConsole" value="$IS_DEBUG" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
</widget>
